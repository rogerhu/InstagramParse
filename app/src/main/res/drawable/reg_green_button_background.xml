<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/green_5_70_transparent" />
            <corners android:radius="@dimen/button_corner_radius_redesign" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent" />
            <stroke android:width="@dimen/button_border_stroke" android:color="@color/green_5_30_transparent" />
            <corners android:radius="@dimen/button_corner_radius_redesign" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/green_5" />
            <corners android:radius="@dimen/button_corner_radius_redesign" />
        </shape>
    </item>
</selector>